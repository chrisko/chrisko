<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Per-commit build of the Yellowstone Frontend server.</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>YSFE_YROOT_NAME</name>
          <description>The (existing!) yroot in which to build YSFE.</description>
          <defaultValue>ysfe</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
    <hudson.plugins.batch__task.BatchTaskProperty>
      <tasks>
        <hudson.plugins.batch__task.BatchTask>
          <name>regenerate_ysfe_yroot</name>
          <script>~/src/ckoenig/hudson/create_or_overwrite_yroot.sh ysfe --i386 4.8-20090518&#xd;
yroot ysfe --cmd &quot;yinst install ~/.hudson/jobs/ysfe/lastStable/archive/pkg/yellowstone_frontend_build-*.tgz -br test&quot;</script>
        </hudson.plugins.batch__task.BatchTask>
      </tasks>
    </hudson.plugins.batch__task.BatchTaskProperty>
    <hudson.plugins.disk__usage.DiskUsageProperty/>
  </properties>
  <scm class="hudson.plugins.git.GitSCM">
    <remoteRepositories>
      <org.spearce.jgit.transport.RemoteConfig>
        <string>origin</string>
        <int>5</int>
        <string>fetch</string>
        <string>+refs/heads/*:refs/remotes/origin/*</string>
        <string>receivepack</string>
        <string>git-upload-pack</string>
        <string>uploadpack</string>
        <string>git-upload-pack</string>
        <string>url</string>
        <string>/home/ckoenig/src/ysfe</string>
        <string>tagopt</string>
        <string></string>
      </org.spearce.jgit.transport.RemoteConfig>
    </remoteRepositories>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/2.1-rel</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <mergeOptions/>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector">
    <hudson.triggers.SCMTrigger>
      <spec>*/1     *     *     *     *</spec>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>git clean -df
yroot $YSFE_YROOT_NAME --cmd &quot;cd $WORKSPACE &amp;&amp; ~/src/ckoenig/hudson/ysfe_build_test_install.sh test&quot;</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.cppunit.CppUnitPublisher>
      <testResultsPattern>tests/test_results.xml</testResultsPattern>
      <useWorkspaceBaseDir>true</useWorkspaceBaseDir>
    </hudson.plugins.cppunit.CppUnitPublisher>
    <hudson.plugins.doxygen.DoxygenArchiver>
      <doxyfilePath>doc/Doxyfile</doxyfilePath>
      <keepAll>false</keepAll>
    </hudson.plugins.doxygen.DoxygenArchiver>
  </publishers>
  <buildWrappers/>
</project>