<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>From-scratch build of the Yellowstone Frontend server.</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>YROOT_IMAGE</name>
          <description>The yroot image in which to build YSFE. See &quot;ysfe --images&quot; for a complete list. The default is RHEL4 u8.</description>
          <defaultValue>4.8-20090518</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>USE_HUDSON_STONECUTTER</name>
          <description>Use a Hudson-built StoneCutter, instead of the disted version specified by the YSFE build package.</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.RunParameterDefinition>
          <name>STONECUTTER_BUILD</name>
          <description>If USE_HUDSON_STONECUTTER above is checked, this can be used to choose a build other than the most recent run.</description>
          <projectName>stonecutter</projectName>
        </hudson.model.RunParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
    <hudson.plugins.disk__usage.DiskUsageProperty/>
  </properties>
  <scm class="hudson.plugins.git.GitSCM">
    <configVersion>1</configVersion>
    <remoteRepositories>
      <org.spearce.jgit.transport.RemoteConfig>
        <string>origin</string>
        <int>5</int>
        <string>fetch</string>
        <string>+refs/heads/*:refs/remotes/origin/*</string>
        <string>receivepack</string>
        <string>git-upload-pack</string>
        <string>uploadpack</string>
        <string>git-upload-pack</string>
        <string>url</string>
        <string>/home/ckoenig/src/ysfe</string>
        <string>tagopt</string>
        <string></string>
      </org.spearce.jgit.transport.RemoteConfig>
    </remoteRepositories>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>**</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <mergeOptions/>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <clean>false</clean>
    <browser>
      <url>http://pathwith.greatamerica.corp.yahoo.com/git/gitweb.cgi/?p=ysfe/.git</url>
    </browser>
    <submoduleCfg class="list"/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>true</disabled>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector">
    <hudson.triggers.SCMTrigger>
      <spec>@hourly</spec>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>git clean -df
~/src/ckoenig/hudson/create_or_overwrite_yroot.sh ysfe_build --i386 $YROOT_IMAGE

# if [[ &quot;$USE_HUDSON_STONECUTTER&quot; = &quot;true&quot; ]]; then
    echo $STONECUTTER_BUILD
# fi

yroot ysfe_build --cmd &quot;cd $WORKSPACE &amp;&amp; ~/src/ckoenig/hudson/ysfe_build_test_install.sh $PACKAGE_BUILD_TYPE&quot;
~/src/ckoenig/hudson/remove_yroot.sh ysfe_build</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>pkg/ysfe*.tgz, tools/yellowstone_*.tgz</artifacts>
      <latestOnly>false</latestOnly>
    </hudson.tasks.ArtifactArchiver>
    <hudson.tasks.BuildTrigger>
      <childProjects>ysfe_deploy</childProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
      </threshold>
    </hudson.tasks.BuildTrigger>
    <hudson.tasks.Fingerprinter>
      <targets></targets>
      <recordBuildArtifacts>true</recordBuildArtifacts>
    </hudson.tasks.Fingerprinter>
    <hudson.plugins.doxygen.DoxygenArchiver>
      <doxyfilePath>doc/Doxyfile</doxyfilePath>
      <keepAll>false</keepAll>
    </hudson.plugins.doxygen.DoxygenArchiver>
  </publishers>
  <buildWrappers>
    <hudson.plugins.release.ReleaseWrapper>
      <releaseVersionTemplate></releaseVersionTemplate>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>PACKAGE_BUILD_TYPE</name>
          <description>By default, test yinst packages will be generated. For release builds, though, which are meant to be uploaded to dist, we&apos;d like to instead build release packages.</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>test</string>
              <string>release</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
      </parameterDefinitions>
      <preBuildSteps/>
      <postBuildSteps/>
    </hudson.plugins.release.ReleaseWrapper>
  </buildWrappers>
</project>